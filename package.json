{
    "name": "@polymedia/react-components",
    "description": "React components for Sui apps",
    "version": "1.2.1",
    "license": "Apache-2.0",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://polymedia.app",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        "source": "./src/index.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
    },
    "scripts": {
        "lint": "eslint ./src --ext .ts,.tsx",
        "build": "rm -rf dist/ && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "prepublishOnly": "pnpm build"
    },
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-react-components"
    },
    "keywords": [
        "polymedia",
        "sui",
        "react",
        "components"
    ],
    "dependencies": {
        "@polymedia/webutils": "^2.3.1"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "eslint": "^8.49.0",
        "eslint-plugin-deprecation": "^1.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.2.2"
    }
}
